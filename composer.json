{
    "name":              "dreamfactory/dfe-partner",
    "description":       "DreamFactory(tm) Enterprise Partner Services Package",
    "homepage":          "https://www.dreamfactory.com/",
    "keywords":          [
        "dreamfactory",
        "dfe",
        "fabric",
        "utility",
        "partner"
    ],
    "license":           [
        "proprietary"
    ],
    "authors":           [
        {
            "name":  "Jerry Ablan",
            "email": "jerryablan@dreamfactory.com"
        }
    ],
    "repositories":      [
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-database.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-common.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-storage.git"
        }
    ],
    "prefer-stable":     false,
    "minimum-stability": "dev",
    "require":           {
        "dreamfactory/dfe-common": "1.0.*"
    },
    "require-dev":       {
        "laravel/framework":           "5.2.*",
        "graham-campbell/flysystem":   "^3.0",
        "league/flysystem-ziparchive": "~1.0",
        "dreamfactory/dfe-database":   "1.0.*",
        "phpunit/phpunit":             "@stable"
    },
    "autoload":          {
        "psr-4": {
            "DreamFactory\\Enterprise\\Partner\\": "src/"
        }
    },
    "config":            {
        "preferred-install": "dist"
    },
    "extra":             {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
